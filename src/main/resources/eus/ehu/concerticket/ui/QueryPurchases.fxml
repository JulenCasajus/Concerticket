          <TableColumn fx:id="q3" editable="false" maxWidth="125.0" minWidth="125.0" prefWidth="125.0" reorderable="false" resizable="false" text="%Tickets"/>
          <TableColumn fx:id="q4" editable="false" maxWidth="125.0" minWidth="125.0" prefWidth="125.0" reorderable="false" resizable="false" text="%Price"/>
      </columns>
        <columnResizePolicy>
